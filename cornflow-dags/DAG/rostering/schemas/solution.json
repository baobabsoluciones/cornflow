{
  "$schema": "http://json-schema.org/schema#",
  "type": "object",
  "properties": {
    "works": {
      "type": "array",
      "description": "Table with the solution for each employee in which time slots has to work",
      "show": true,
      "filterable": true,
      "sortable": true,
      "items": {
        "type": "object",
        "properties": {
          "id_employee": {
            "type": "integer",
            "description": "The unique identifier for each employee.",
            "title": "Employee id.",
            "sortable": true,
            "filterable": true
          },
          "time_slot": {
            "type": "string",
            "title": "Time slot",
            "description": "The time slot the employee has to work on",
            "sortable": true,
            "filterable": true
          }
        },
        "required": [
          "id_employee",
          "time_slot"
        ]
      }
    },
    "indicators": {
      "type": "object",
      "description": "Indicators of solution",
      "show": true,
      "properties": {
        "objective_function": {
          "type": "number",
          "title": "OF",
          "description": "Objective function"
        },
        "one_employee": {
          "type": "number",
          "title": "One employee %",
          "description": "Percentage of time slots with only one employee"
        },
        "mean_demand": {
          "type": "number",
          "title": "Mean demand",
          "description": "Mean demand attended by employees"
        },
        "max_demand": {
          "type": "number",
          "title": "Max demand",
          "description": "Max demand attended by employees"
        },
        "min_demand": {
          "type": "number",
          "title": "Min demand",
          "description": "Min demand attended by employees"
        }
      },
      "required": []
    }
  },
  "required": [
    "works"
  ]
}