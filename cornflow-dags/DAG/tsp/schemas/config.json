{
    "$schema": "http://json-schema.org/schema#",
    "type": "object",
    "properties": {
        "timeLimit": {"type": "number"},
        "threads": {"type": "integer"},
        "seed": {"type": "integer"},
        "gap": {"type": "number"},
        "report": {
            "type": "object",
            "properties": {
                "name": {"type":  "string"}
            }
        },
        "solver": {
            "type": "string",
            "enum": ["naive", "cpsat"],
            "default": "naive"
        }
    }
}
