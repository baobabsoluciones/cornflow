{
	"$schema": "http://json-schema.org/schema#",
	"type": "object",
	"properties": {
		"routes": {
			"description": "Order of nodes in each route",
			"type": "array",
			"items": {
				"type": "object",
				"properties": {
					"route": {
						"type": "integer"
					},
					"node": {
						"type": "integer"
					},
					"pos": {
						"type": "integer"
					}
				},
				"required": [
					"route",
					"pos",
					"node"
				]
			}
		}
	},
	"required": [
		"routes"
	]
}